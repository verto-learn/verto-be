  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start:server": "node dist/src/server.js",
    "start:worker": "node dist/src/worker.js",
    "start": "concurrently \"npm run start:server\" \"npm run start:worker\"",
    "dev": "concurrently \"npx ts-node src/server.ts\" \"npx ts-node src/worker.ts\"",
    "dev:worker": "ts-node src/worker.ts",
    "dev:nodemon": "nodemon --watch 'src/**/*.ts' --exec ts-node src/server.ts",
    "seed": "ts-node prisma/seed.ts"
  },
